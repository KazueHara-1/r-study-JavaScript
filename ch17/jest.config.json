{
  "preset": "ts-jest/presets/default-esm",
  "testMatch": ["**/?(*.)+(test).+(js|jsx)"],
  "transform": {
    "^.+\\.(js|jsx)$": "babel-jest",
    "^.+\\.(ts|tsx)$": "ts-jest",
    "^.+\\.mjs$": "babel-jest"
  },
  "transformIgnorePatterns": ["/node_modules/(?!@octokit).+\\.js"],
  "testEnvironment": "jsdom"
}
